{
  "name": "spiderman",
  "description": "m.youxiang0210.com",
  "author": "youxiang0210 frontend",
  "private": true,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "start-pm2": "cross-env NODE_ENV=production pm2 start server.js --name spiderman -i 0",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:dev": "rimraf dist && npm run build:client-dev && npm run build:server-dev",
    "build:test": "rimraf dist && BUILD_ENV=test npm run build:client && BUILD_ENV=test npm run build:server",
    "build:pre": "rimraf dist && BUILD_ENV=pre npm run build:client && BUILD_ENV=pre npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:client-dev": "cross-env NODE_ENV=development webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server-dev": "cross-env NODE_ENV=development webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-preset-env": "^1.6.1",
    "compression": "^1.6.2",
    "cross-env": "^4.0.0",
    "es6-promise": "^4.1.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "firebase": "^3.7.2",
    "lru-cache": "^4.0.2",
    "moment": "^2.22.1",
    "precss": "^1.4.0",
    "scss-loader": "^0.0.1",
    "serve-favicon": "^2.4.1",
    "url-parse": "^1.2.0",
    "video.js": "^5.20.5",
    "videojs-contrib-hls": "^5.5.3",
    "vue": "^2.3.2",
    "vue-awesome-swiper": "^2.4.2",
    "vue-infinite-loading": "^2.3.1",
    "vue-lazyload": "^1.0.4",
    "vue-router": "^2.5.0",
    "vue-server-renderer": "^2.3.2",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.3",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.1",
    "rimraf": "^2.6.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "sw-precache-webpack-plugin": "^0.10.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.0.2",
    "vue-style-loader": "^3.0.0",
    "vue-template-compiler": "^2.3.2",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.5.4"
  }
}
